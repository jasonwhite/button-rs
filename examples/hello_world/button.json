[
    {
        "inputs": ["foo.c", "foo.h"],
        "tasks": [
            {
                "type": "command",
                "program": "gcc",
                "args": ["-c", "foo.c", "-o", "foo.o"]
            }
        ],
        "outputs": ["foo.o"]
    },
    {
        "inputs": ["bar.c", "foo.h"],
        "tasks": [
            {
                "type": "command",
                "program": "gcc",
                "args": ["-c", "bar.c", "-o", "bar.o"]
            }
        ],
        "outputs": ["bar.o"]
    },
    {
        "inputs": ["foo.o", "bar.o"],
        "tasks": [
            {
                "type": "command",
                "program": "gcc",
                "args": ["foo.o", "bar.o", "-o", "foobar"]
            }
        ],
        "outputs": ["foobar"]
    }
]
